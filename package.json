{
  "name": "thinkt-web",
  "version": "0.1.0",
  "description": "THINKT API web application - project and session browser",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run lint && tsc && vite build",
    "build:fast": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "api:convert": "swagger2openapi src/api/swagger.json -o src/api/openapi.json",
    "api:generate": "npm run api:convert && openapi-typescript src/api/openapi.json -o src/api/generated.ts"
  },
  "author": "Neomantra Corp",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "jsdom": "^27.4.0",
    "openapi-typescript": "^7.10.1",
    "swagger2openapi": "^7.0.8",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.54.0",
    "vite": "^6.0.0",
    "vitest": "^2.1.0"
  },
  "dependencies": {
    "@wethinkt/ts-thinkt": "file:../ts-thinkt"
  }
}
